{
  "schemaVersion": 1,
  "id": "darkarch",
  "version": "${version}",
  "name": "Dark Arch",
  "description": "A Fabric magic mod that makes your Minecraft darker",
  "authors": ["toby7002"],
  "contact": {
    "sources": "https://github.com/toby7002/DarkArch"
  },
  "license": "MIT",
  "icon": "assets/darkarch/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": ["darkarch.DADataGen"],
    "main": [
      {
        "value": "darkarch.DarkArch",
        "adapter": "kotlin"
      }
    ]
  },
  "mixins": ["darkarch.mixins.json"],
  "depends": {
    "fabricloader": ">=0.14.22",
    "minecraft": "~1.20.2",
    "java": ">=17",
    "fabric-api": "*",
    "fabric-language-kotlin": ">=1.9.10",
    "owo-lib": "*",
    "trinkets": "*",
    "geckolib": "*"
  }
}
